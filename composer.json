{
    "name": "sensio/distribution-bundle",
    "description": "Base bundle for Symfony Distributions",
    "keywords": ["distribution","configuration"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:keichinger/security-checker.git"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/dependency-injection": "^4.0.0-BETA1",
        "symfony/config": "^4.0.0-BETA1",
        "symfony/filesystem": "^4.0.0-BETA1",
        "symfony/http-kernel": "^4.0.0-BETA1",
        "symfony/class-loader": "^3.4.0-BETA1",
        "symfony/process": "^4.0.0-BETA1",
        "sensiolabs/security-checker": "dev-master#360721f77cbdab9ca3f64e586524d6f64f9bef12"
    },
    "autoload": {
        "psr-4": { "Sensio\\Bundle\\DistributionBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}
